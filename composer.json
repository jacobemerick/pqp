{
  "name": "particletree/pqp",
  "description": "Simple PHP profiler to track application behavior during development",
  "license": "MIT",
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "phpunit/phpunit": "^4.7",
    "symfony/config": "<3.0",
    "symfony/console": "<3.0",
    "symfony/filesystem": "<3.0",
    "symfony/stopwatch": "<3.0",
    "symfony/yaml": "<3.0"
  },
  "autoload": {
    "psr-4": {
      "Particletree\\Pqp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Particletree\\Pqp\\": "tests/unit/"
    }
  }
}
